    <connection>neo4j-db</connection>
    <cypher>MATCH(n:Document) 
WHERE date(n.node_updated_on)  &lt;  date({retention_date}) AND NOT EXISTS (n.masked)
WITH n 
SET n.number = randomUUID(),
n.exp_date = date(),
n.masked = 'Y',
n.maksed_on = datetime()</cypher>
